<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox prefHeight="144.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.StartScreen">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="121.0" prefWidth="246.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="chooseFile" layoutX="167.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#newFile" text="Choose" />
            <Label layoutX="32.0" layoutY="14.0" prefHeight="26.0" prefWidth="119.0" text="Choose File:" />
            <TextField fx:id="chosenFile" layoutX="26.0" layoutY="66.0" prefHeight="26.0" prefWidth="206.0" />
            <Button fx:id="fileOK" layoutX="26.0" layoutY="100.0" mnemonicParsing="false" onAction="#chooseFile" prefHeight="26.0" prefWidth="206.0" text="OK" />
         </children>
      </AnchorPane>
   </children>
</VBox>
